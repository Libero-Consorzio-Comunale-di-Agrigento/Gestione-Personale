FILE=GP4am_ins.sql
SET TERMOUT OFF
TABLE=a_menu where voce_menu in (select voce_menu from a_voci_menu where voce_menu like 'PAM%' union select distinct voce_menu from a_guide_o where guida_o in (select distinct guida_o from a_voci_menu where voce_menu like 'PAM%') and voce_menu not like 'PAM%' union select distinct voce_menu from a_guide_v where guida_v in (select distinct guida_v from a_guide_o where guida_o in (select distinct guida_o from a_voci_menu where voce_menu like 'PAM%' )) and voce_menu not like 'PAM%') and ruolo = 'AMM' and applicazione = 'GP4'
TABLE=a_menu where voce_menu in (select voce_menu from a_voci_menu where voce_menu like 'PXX%' union select distinct voce_menu from a_guide_o where guida_o in (select distinct guida_o from a_voci_menu where voce_menu like 'PXX%') and voce_menu not like 'PXX%' union select distinct voce_menu from a_guide_v where guida_v in (select distinct guida_v from a_guide_o where guida_o in (select distinct guida_o from a_voci_menu where voce_menu like 'PXX%' )) and voce_menu not like 'PXX%') and ruolo = 'AMM' and applicazione = 'GP4'
TABLE=a_menu where ruolo = 'AMM' and applicazione = 'GP4' and voce_menu in (select voce_menu from a_voci_menu where voce_menu like 'PAA%') 
TABLE=a_menu where voce_menu in (select voce_menu from a_voci_menu where voce_menu like 'P00%' union select distinct voce_menu from a_guide_o where guida_o in (select distinct guida_o from a_voci_menu where voce_menu like 'P00%') and voce_menu not like 'P00%' union select distinct voce_menu from a_guide_v where guida_v in (select distinct guida_v from a_guide_o where guida_o in (select distinct guida_o from a_voci_menu where voce_menu like 'P00%' )) and voce_menu not like 'P00%') and ruolo = 'AMM' and applicazione = 'GP4'
